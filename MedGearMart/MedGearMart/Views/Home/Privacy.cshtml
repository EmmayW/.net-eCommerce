@{
    ViewData["Title"] = "Our Members";
}
<h1 class="text-center mb-5">@ViewData["Title"]</h1>

<div class="container">
    <div class="row">
        <!-- Card 1 -->
        <div class="col-md-6 mb-4">
            <div class="card">
                <img src="~/images/member1.jpg" class="card-img-top" alt="Member 1">
                <div class="card-body">
                    <h5 class="card-title text-center">John Doe</h5>
                </div>
            </div>
        </div>
        <!-- Card 2 -->
        <div class="col-md-6 mb-4">
            <div class="card">
                <img src="~/images/member2.jpg" class="card-img-top" alt="Member 2">
                <div class="card-body">
                    <h5 class="card-title text-center">Jane Smith</h5>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <!-- Card 3 -->
        <div class="col-md-6 mb-4">
            <div class="card">
                <img src="~/images/member3.jpg" class="card-img-top" alt="Member 3">
                <div class="card-body">
                    <h5 class="card-title text-center">Alice Johnson</h5>
                </div>
            </div>
        </div>
        <!-- Card 4 -->
        <div class="col-md-6 mb-4">
            <div class="card">
                <img src="~/images/member4.jpg" class="card-img-top" alt="Member 4">
                <div class="card-body">
                    <h5 class="card-title text-center">Bob Brown</h5>
                </div>
            </div>
        </div>
    </div>
</div>
